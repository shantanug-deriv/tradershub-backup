  -webkit-box-pack:center;
      -ms-flex-pack:center;
          justify-content:center;
}
[style*="text-align: right"] .vertical-align{
  -webkit-box-pack:end;
      -ms-flex-pack:end;
          justify-content:flex-end;
}
