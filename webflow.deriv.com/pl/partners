<script>
  // Parse the cookies to get the 'client_information' cookie value
  const cookieValue = window.parseCookies(document.cookie, "client_information")
    ? JSON.parse(window.parseCookies(document.cookie, "client_information"))
    : null; 

  // Hotjar integration
